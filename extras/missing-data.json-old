            "sourceEnum": 1,
            "source": "Archetype",
            "sourceName": "Way of Suggestion",
            "metadata": null,
            "partitionKey": "Core",
            "rowKey": "Archetype-Way of Suggestion-Subtle Presence-18",
            "timestamp": "?",
            "eTag": "?"
        },
