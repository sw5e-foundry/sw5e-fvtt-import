    "EnhancedItem.name-mandalorian_beskargam.subtype-any": {
        "name": "Mandalorian Beskar'gam",
        "flags": {
            "sw5e-importer": {
                "timestamp": "2023-03-09T01:52:04.8484415+00:00",
