    "EnhancedItem.name-orbalisk_armor.subtype-heavy_exoskeleton": {
        "name": "Orbalisk Armor",
        "flags": {
            "sw5e-importer": {
                "timestamp": "2023-03-09T01:52:04.8484415+00:00",
