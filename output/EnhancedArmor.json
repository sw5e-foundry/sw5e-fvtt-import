    "EnhancedItem.name-matrix_armor.subtype-any_heavy": {
        "name": "Matrix Armor",
        "flags": {
            "sw5e-importer": {
                "timestamp": "2023-03-09T01:52:04.8484415+00:00",
                "importer_version": 3,
                "uid": "EnhancedItem.name-matrix_armor.subtype-any_heavy"
            }
        },
        "type": "equipment",
        "img": "systems/sw5e/packs/Icons/Enhanced%20Items/MatrixArmor.webp",
        "system": {
            "description": {
                "value": "<h4>Armor (Any Heavy)</h4>\n <p>While wearing this suit of armor, any critical hit against you becomes a normal hit.</p>\n<p><em>This suit of armor has had its protective durasteel components replaced by equivalents made of matrix, a purple metal used to reinforce starships.</em></p>"
            },
